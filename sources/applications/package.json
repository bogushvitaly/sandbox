  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected.apps": "nx affected:apps",
    "affected.build": "nx affected:build",
    "affected.dep-graph": "nx affected:dep-graph",
    "affected.e2e": "nx affected:e2e",
    "affected.libs": "nx affected:libs",
    "affected.lint": "nx affected:lint",
    "affected.test": "nx affected:test",
    "analyze.web.application.bundle": "ng build web-application --prod --stats-json --build-optimizer=false --vendor-chunk=true && webpack-bundle-analyzer dist/apps/web-application/browser/stats.json --no-open --mode static --report reports/apps/web-application/bundle/index.html",
    "analyze.web.application.lighthouse": "start-server-and-test start.nest.application http://localhost:4000 analyze.web.application.lighthouse.run",
    "analyze.web.application.lighthouse.run": "make-dir reports/apps/web-application/lighthouse && lighthouse http://localhost:4000 --config-path=./lighthouse.config.js --output=html --output-path=./reports/apps/web-application/lighthouse/index.html --save-assets",
    "build": "ng build",
    "build.web.application": "run-s build.web.application.browser build.web.application.server",
    "build.web.application.browser": "cross-env-shell ng build web-application --configuration=$CI_ENVIRONMENT_NAME",
    "build.web.application.browser.watch": "ng build web-application --watch",
    "build.web.application.server": "ng run web-application:server:production",
    "build.web.application.server.watch": "ng run web-application:server --watch",
    "build.web.application.watch": "run-p build.web.application.browser.watch build.web.application.server.watch",
    "clean": "pnpx rimraf hooks node_modules shrinkwrap.yaml && npm cache clean --force && npm run update.deps && pnpm install",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "dep-graph": "node ./node_modules/.registry.npmjs.org/@nrwl/schematics/7.5.0/node_modules/@nrwl/schematics/src/command-line/nx.js dep-graph --all --file=./reports/deps-graph/index.html",
    "deploy.web.application": "sync-files --delete --verbose dist/apps functions/dist/apps && cd functions && firebase deploy",
    "docs.application": "hads ./docs -o",
    "docs.compodoc": "compodoc -p ./tsconfig.json -d reports/compodoc",
    "e2e": "ng e2e",
    "e2e.codecept.init": "cd apps/REPLACE_WITH_APPLICATION_NAME && codeceptjs init && codeceptjs gherkin:init && codeceptjs def .",
    "e2e.web.application": "start-server-and-test start.web.application http-get://localhost:4200 e2e.web.application.report",
    "e2e.web.application.report": "npm run e2e.web.application.run && npm run e2e.web.application.report.allure",
    "e2e.web.application.report.allure": "allure generate reports/apps/web-application/codecept --clean --report-dir reports/apps/web-application/allure",
    "e2e.web.application.run": "codeceptjs run --plugins allure --plugins stepByStepReport --reporter mocha-multi --config apps/web-application-e2e/codecept.conf.js",
    "e2e.web.application.shell": "cd apps/web-application-e2e && codeceptjs shell",
    "e2e.web.application.steps": "codeceptjs gherkin:snippets --config apps/web-application-e2e/codecept.conf.js",
    "e2e.web.application.watch": "wait-on http-get://localhost:4200 && nodemon --exec \"npm run e2e.web.application.steps && npm run e2e.web.application.run\"",
    "format": "nx format:write",
    "format.check": "nx format:check",
    "format.write": "nx format:write",
    "help": "nx help",
    "postinstall": "node tools/web/postinstall && npm run packages.patch && ts-node tools/scripts/generate-version",
    "lint.check": "npm run lint.ng && npm run lint.html && npm run lint.scss",
    "lint.html": "htmlhint ./{apps,libs}/**/*.html",
    "prelint.ng": "renamer --find tslint.json --replace tslint-nx.json * && renamer --find tslint-base.json --replace tslint.json *",
    "lint.ng": "ng lint",
    "postlint.ng": "renamer --find tslint.json --replace tslint-base.json * && renamer --find tslint-nx.json --replace tslint.json *",
    "lint.nx": "nx lint",
    "lint.scss": "stylelint ./{apps,libs}/**/*.scss --syntax scss",
    "lint.write": "npm run format && npm run lint.ng -- --fix && npm run lint.html && npm run lint.scss -- --fix",
    "mock.api": "json-server",
    "ng": "ng",
    "packages.patch": "npx babel tools/custom-packages/babylonjs-mesh-writer/meshwriter.js --out-file tools/custom-packages/babylonjs-mesh-writer/babylonjs.meshWriter.js --plugins=transform-amd-to-commonjs",
    "postpackages.patch": "sync-files tools/custom-packages/babylonjs-mesh-writer node_modules/babylonjs-mesh-writer",
    "postinstall.windows": "npm rebuild node-sass",
    "precommit": "sort-package-json && npm run lint",
    "prettier": "prettier --write ./{apps,libs}/**/*.{ts,js,scss}",
    "prettier.check": "prettier --list-different ./{apps,libs}/**/*.{ts,js,scss}",
    "report.application.web.open": "sync-files --no-delete tools/report-wrapper reports/apps/web-application && ws --directory reports/apps/web-application --open",
    "start": "ng serve",
    "start.web.application": "ng serve web-application --aot",
    "start.web.application.atdd": "npm-run-all -p --print-label start.web.application.hmr e2e.web.application.watch test.libs.watch test.xplat.watch test.web.application.watch",
    "start.web.application.hmr": "cross-env DEVELOPMENT_MODE=true ng serve web-application --hmr",
    "storybook": "npm run storybook.prebuild && start-storybook -p 9008 -s apps/storybook/src/assets",
    "storybook.build": "npm run storybook.prebuild && build-storybook -s apps/storybook/src/assets -o reports/storybook",
    "storybook.open": "ws --directory storybook-static --open",
    "storybook.prebuild": "npm run test.jest.generate-output",
    "test": "run-s test.libs.coverage test.xplat.coverage test.web.application.coverage",
    "test.jest.coverage": "jest --config=jest.addon-config.js --coverage --coverageDirectory=reports/coverage",
    "test.jest.generate-output": "jest --json --config=jest.addon-config.js --outputFile=addon-jest.testresults.json",
    "test.jest.watch": "jest --json --config=jest.addon-config.js --watchAll",
    "test.libs.coverage": "ng test libs --no-watch --code-coverage",
    "test.libs.watch": "ng test libs --watch",
    "test.watch": "npm-run-all -p --print-label test.libs.watch test.xplat.watch test.web.application.watch",
    "test.web.application.coverage": "ng test web-application --no-watch --code-coverage",
    "test.web.application.watch": "ng test web-application --watch",
    "test.xplat.coverage": "ng test xplat --no-watch --code-coverage",
    "test.xplat.watch": "ng test xplat --watch",
    "translations.web.application": "ngx-translate-extract --input apps/web-application/src --output apps/web-application/src/assets/i18n/template.json --format=json --clean -sort --marker extract",
    "udk.web.application": "cross-env DEVELOPMENT_MODE=true ng-udkc --project web-application",
    "preudk.web.application.build": "ts-node tools/scripts/patch-environment",
    "udk.web.application.build": "ng run web-application:udk:production",
    "udk.web.application.start": "node ./dist/apps/web-application/server/main.js",
    "update": "ng update @nrwl/schematics",
    "update.check": "ng update",
    "update.deps": "npm run update.deps.base && npm run update.deps.functions",
    "update.deps.base": "pnpx sort-package-json && pnpx npm-check-updates -u && pnpm install --prefer-offline",
    "preupdate.deps.functions": "ts-node tools/scripts/split-dependencies",
    "update.deps.functions": "cd functions && pnpx sort-package-json && pnpx npm-check-updates -u && pnpm install --prefer-offline",
    "workspace-schematic": "nx workspace-schematic"
  },

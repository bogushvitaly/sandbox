{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "analyze.web.application.bundle": "ng build web-application --prod --stats-json --build-optimizer=false --vendor-chunk=true && webpack-bundle-analyzer dist/apps/web-application/stats.json --no-open --mode static --report reports/web-application/bundle/index.html",
    "analyze.web.application.lighthouse": "start-server-and-test start.web.application.ssr http://localhost:4000 analyze.web.application.lighthouse.run",
    "analyze.web.application.lighthouse.run": "make-dir reports/web-application/lighthouse && lighthouse-ci http://localhost:4000 --report reports/web-application/lighthouse",
    "build": "ng build",
    "build.nest.application": "tsc -p apps/nest-application",
    "build.web.application": "run-s build.web.application.browser build.web.application.server build:web.application.ssr",
    "build.web.application.browser": "cross-env-shell ng build web-application --configuration=$CI_ENVIRONMENT_NAME",
    "build.web.application.browser.watch": "ng build web-application --watch",
    "build.web.application.server": "ng run web-application:server:production",
    "build.web.application.server.watch": "ng run web-application:server --watch",
    "build.web.application.ssr": "ng build web-application-ssr --prod",
    "build.web.application.watch": "npm-run-all -p build.web.application.browser.watch build.web.application.server.watch",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm cache clean --force && npm i --force",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "dep-graph": "nx dep-graph -- --file=dep-graph.json",
    "deploy": "",
    "deploy.web.application": "sync-files dist/apps/web-application functions/dist/apps/web-application && sync-files dist/apps/web-application-server functions/dist/apps/web-application-server && cd functions && firebase use --token $FIREBASE_TOKEN $CI_ENVIRONMENT_NAME && npm run deploy",
    "deploy.web.application.patch": "node tools/server-externals/package-util.js",
    "docker": "npm run docker.build && npm run docker.run",
    "docker.build": "docker build -f Dockerfile.build -t bohushvitali/sandbox-frontend:latest --rm .",
    "docker.build.dist": "docker build -f Dockerfile -t bohushvitali/sandbox-frontend-dist:latest --rm .",
    "docker.push": "docker push bohushvitali/sandbox-frontend:latest",
    "docker.run": "docker run -it --rm -p 4000:4000 -e PORT=4000 --name sandbox-frontend bohushvitali/sandbox-frontend:latest",
    "docker.run.dist": "docker run -it --rm -p 4000:4000 -e PORT=4000 --name sandbox-frontend bohushvitali/sandbox-frontend-dist:latest",
    "docs.application": "hads ./docs -o",
    "docs.compodoc": "compodoc -p ./tsconfig.json -s",
    "e2e": "ng e2e",
    "e2e.codecept.init": "cd apps/REPLACE_WITH_APPLICATION_NAME && codeceptjs init && codeceptjs gherkin:init && codeceptjs def .",
    "e2e.web.application": "start-server-and-test start.web.application http-get://localhost:4200 e2e.web.application.run",
    "e2e.web.application.report": "npm run e2e.web.application.run -- --reporter mochawesome && npm run e2e.web.application.run -- --reporter mocha-junit-reporter && allure generate reports/web-application/codecept --clean --report-dir reports/web-application/allure",
    "e2e.web.application.run": "codeceptjs run --config apps/web-application-e2e/codecept.conf.js",
    "e2e.web.application.shell": "cd apps/web-application-e2e && codeceptjs shell",
    "e2e.web.application.steps": "codeceptjs gherkin:snippets --config apps/web-application-e2e/codecept.conf.js",
    "e2e.web.application.watch": "wait-on http-get://localhost:4200 && nodemon --exec \"npm run e2e.web.application.steps && npm run e2e.web.application.run\"",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "preinstall": "npx sort-package-json && npx npm-check-updates -u",
    "postinstall": "node tools/web/postinstall && ts-node tools/generate-version",
    "lint": "npm run format && npm run lint.html && npm run lint.scss && nx lint && ng lint --fix",
    "lint.html": "htmlhint ./{apps,libs}/**/*.html",
    "lint.scss": "stylelint ./{apps,libs}/**/*.scss --syntax scss --fix",
    "mock.api": "json-server --watch",
    "ng": "ng",
    "precommit": "sort-package-json && npm run lint",
    "prettier": "prettier --write ./{apps,libs}/**/*.{ts,js,scss}",
    "prettier:check": "prettier --list-different ./{apps,libs}/**/*.{ts,js,scss}",
    "report.application.web.open": "sync-files --no-delete tools/report-wrapper reports/web-application && ws --directory reports/web-application --open",
    "serve.nest.application": "ts-node -P apps/nest-application/tsconfig.json apps/nest-application/src/main.ts",
    "start": "ng serve",
    "start.nest.application": "npm-run-all -p serve.nest.application",
    "start.web.application": "ng serve web-application --aot",
    "start.web.application.atdd": "npm-run-all --print-label --parallel start.web.application.hmr test.web.application.watch e2e.web.application.watch",
    "start.web.application.hmr": "ng serve web-application --hmr",
    "start.web.application.ssr": "node dist/apps/web-application-ssr/main",
    "storybook": "npm run storybook.prebuild && start-storybook -p 9008 -s apps/web-application/src/assets",
    "storybook.build": "npm run storybook.prebuild && build-storybook -s apps/web-application/src/assets",
    "storybook.open": "ws --directory storybook-static --open",
    "storybook.prebuild": "npm run test.generate-output",
    "test": "run-s test.libs.coverage test.xplat.coverage test.web.application.coverage",
    "test.generate-output": "jest --json --config=jest.addon-config.js --outputFile=addon-jest.testresults.json || true",
    "test.coverage": "jest --config=jest.addon-config.js --coverage --coverageDirectory=reports/coverage",
    "test.libs.coverage": "ng test libs --no-watch --code-coverage",
    "test.nest.application": "jest --config=apps/nest-application/jest.json",
    "test.nest.application.coverage": "jest --config=apps/nest-application/jest.json --coverage --coverageDirectory=coverage",
    "test.nest.application.e2e": "jest --config=apps/nest-application/e2e/jest-e2e.json --forceExit",
    "test.nest.application.e2e.watch": "jest --config=apps/nest-application/e2e/jest-e2e.json --watch",
    "test.nest.application.watch": "jest --config=apps/nest-application/jest.json --watch",
    "test.web.application.coverage": "ng test --project=web-application --no-watch --code-coverage",
    "test.web.application.watch": "ng test --project=web-application --watch",
    "test.xplat.coverage": "ng test xplat --no-watch --code-coverage",
    "translations.web.application": "ngx-translate-extract --input apps/web-application/src --output apps/web-application/src/assets/i18n/template.json --format=json --clean -sort --marker extract",
    "udk.web.application": "ng-udkc --project web-application",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic"
  },

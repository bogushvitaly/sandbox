{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "ng build",
    "build.nest.application": "tsc -p apps/nest-application",
    "build.web.application": "run-s build.web.application.browser build.web.application.server build:web.application.ssr",
    "build.web.application.browser": " ng build web-application --prod",
    "build.web.application.browser.watch": " ng build web-application --watch",
    "build.web.application.server": "ng run web-application:server:production",
    "build.web.application.server.watch": "ng run web-application:server --watch",
    "build:web.application.ssr": "ng build web-application-ssr --prod",
    "build:web.application.watch": "npm-run-all -p build.web.application.browser.watch build.web.application.server.watch",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "dep-graph": "nx dep-graph",
    "deploy": "",
    "docker": "npm run docker:build && npm run docker:run",
    "docker:build": "docker build -f Dockerfile.build -t bohushvitali/sandbox-frontend:latest --rm .",
    "docker:build:dist": "docker build -f Dockerfile -t bohushvitali/sandbox-frontend-dist:latest --rm .",
    "docker:push": "docker push bohushvitali/sandbox-frontend:latest",
    "docker:run": "docker run -it --rm -p 4000:4000 -e PORT=4000 --name sandbox-frontend bohushvitali/sandbox-frontend:latest",
    "docker:run:dist": "docker run -it --rm -p 4000:4000 -e PORT=4000 --name sandbox-frontend bohushvitali/sandbox-frontend-dist:latest",
    "docs": "hads ./docs -o",
    "e2e": "ng e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "ts-node tools/generate-version",
    "lint": "nx lint && ng lint --fix",
    "ng": "ng",
    "serve.nest.application": "ts-node -P apps/nest-application/tsconfig.json apps/nest-application/src/main.ts",
    "start": "ng serve",
    "start.nest.application": "npm-run-all -p serve.nest.application",
    "start.web.application": "ng serve web-application",
    "start.web.application.ssr": "node dist/apps/web-application-ssr/main",
    "test": "ng test",
    "test.nest.application": "jest --config=apps/nest-application/jest.json",
    "test.nest.application.coverage": "jest --config=apps/nest-application/jest.json --coverage --coverageDirectory=coverage",
    "test.nest.application.e2e": "jest --config=apps/nest-application/e2e/jest-e2e.json --forceExit",
    "test.nest.application.e2e.watch": "jest --config=apps/nest-application/e2e/jest-e2e.json --watch",
    "test.nest.application.watch": "jest --config=apps/nest-application/jest.json --watch",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "precommit": "sort-package-json && npm run lint"
  },

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/bin/opencollective.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/bin/opencollective.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/node_modules" "$basedir/node"  "$basedir/../opencollective/dist/bin/opencollective.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/bin/opencollective.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules/opencollective/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/1.0.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/opencollective/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/bin/opencollective.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/opencollective/node_modules" node  "$basedir/../opencollective/dist/bin/opencollective.js" "$@"

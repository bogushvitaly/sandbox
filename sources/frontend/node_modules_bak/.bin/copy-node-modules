#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/bin/copy-node-modules.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/bin/copy-node-modules.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/node_modules" "$basedir/node"  "$basedir/../copy-node-modules/bin/copy-node-modules.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/bin/copy-node-modules.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules/copy-node-modules/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/1.1.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/copy-node-modules/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/bin/copy-node-modules.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/copy-node-modules/node_modules" node  "$basedir/../copy-node-modules/bin/copy-node-modules.js" "$@"
  ret=$?
fi

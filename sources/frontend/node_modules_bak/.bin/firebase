#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/bin/firebase.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/bin/firebase.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/node_modules" "$basedir/node"  "$basedir/../firebase-tools/lib/bin/firebase.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/bin/firebase.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules/firebase-tools/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/6.3.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/firebase-tools/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/bin/firebase.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/firebase-tools/node_modules" node  "$basedir/../firebase-tools/lib/bin/firebase.js" "$@"

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/npm-run-all/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/npm-run-all/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/node_modules" "$basedir/node"  "$basedir/../npm-run-all/bin/npm-run-all/index.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/npm-run-all/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/4.1.5/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/npm-run-all/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/npm-run-all/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run-all/node_modules" node  "$basedir/../npm-run-all/bin/npm-run-all/index.js" "$@"

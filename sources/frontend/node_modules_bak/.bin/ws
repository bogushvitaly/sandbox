#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/node_modules" "$basedir/node"  "$basedir/../local-web-server/bin/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules/local-web-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/2.6.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/local-web-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/local-web-server/node_modules" node  "$basedir/../local-web-server/bin/cli.js" "$@"
  ret=$?
fi
exit $ret

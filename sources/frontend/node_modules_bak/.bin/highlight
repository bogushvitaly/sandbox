#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/bin/highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/bin/highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/node_modules" "$basedir/node"  "$basedir/../cli-highlight/bin/highlight" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/bin/highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules/cli-highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/1.2.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cli-highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/bin/highlight/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cli-highlight/node_modules" node  "$basedir/../cli-highlight/bin/highlight" "$@"
  ret=$?
fi
exit $ret

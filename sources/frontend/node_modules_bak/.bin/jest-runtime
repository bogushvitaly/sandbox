#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/node_modules" "$basedir/node"  "$basedir/../jest-runtime/bin/jest-runtime.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules/jest-runtime/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/23.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/jest-runtime/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/jest-runtime/node_modules" node  "$basedir/../jest-runtime/bin/jest-runtime.js" "$@"
  ret=$?
fi
exit $ret

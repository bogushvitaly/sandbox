#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/bin/npm-run.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/bin/npm-run.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/node_modules" "$basedir/node"  "$basedir/../npm-run/bin/npm-run.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/bin/npm-run.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules/npm-run/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/4.1.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/npm-run/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/bin/npm-run.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/npm-run/node_modules" node  "$basedir/../npm-run/bin/npm-run.js" "$@"
  ret=$?
fi
exit $ret

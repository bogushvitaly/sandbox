#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/bin/cross-env.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/bin/cross-env.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/node_modules" "$basedir/node"  "$basedir/../cross-env/dist/bin/cross-env.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/bin/cross-env.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules/cross-env/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/5.2.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/cross-env/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/bin/cross-env.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/cross-env/node_modules" node  "$basedir/../cross-env/dist/bin/cross-env.js" "$@"
  ret=$?

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/src/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/src/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/src/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/src/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/node_modules" "$basedir/node"  "$basedir/../@angular/compiler-cli/src/main.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/src/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/src/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/7.2.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/src/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/src/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/compiler-cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/node_modules" node  "$basedir/../@angular/compiler-cli/src/main.js" "$@"

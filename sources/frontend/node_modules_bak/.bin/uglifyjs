#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/node_modules" "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules/uglify-js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/3.4.9/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/uglify-js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/uglify-js/node_modules" node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret

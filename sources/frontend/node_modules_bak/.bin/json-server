#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/cli/bin.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/cli/bin.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/node_modules" "$basedir/node"  "$basedir/../json-server/lib/cli/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/cli/bin.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules/json-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/0.14.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/json-server/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/cli/bin.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/json-server/node_modules" node  "$basedir/../json-server/lib/cli/bin.js" "$@"

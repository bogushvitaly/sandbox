else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/graphql-schema-linter/0.2.0/node_modules/graphql-schema-linter/lib/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/graphql-schema-linter/0.2.0/node_modules/graphql-schema-linter/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/graphql-schema-linter/0.2.0/node_modules/graphql-schema-linter/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/graphql-schema-linter/0.2.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/graphql-schema-linter/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/graphql-schema-linter/lib/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/graphql-schema-linter/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/graphql-schema-linter/node_modules" node  "$basedir/../graphql-schema-linter/lib/cli.js" "$@"

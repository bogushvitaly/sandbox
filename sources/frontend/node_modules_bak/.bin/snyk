#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/cli/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/cli/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/node_modules" "$basedir/node"  "$basedir/../snyk/dist/cli/index.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/cli/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules/snyk/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/1.122.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/snyk/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/cli/index.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/dist/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/snyk/node_modules" node  "$basedir/../snyk/dist/cli/index.js" "$@"

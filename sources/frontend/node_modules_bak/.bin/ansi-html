#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/bin/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/node_modules" "$basedir/node"  "$basedir/../ansi-html/bin/ansi-html" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/0.0.7/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/bin/ansi-html/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ansi-html/node_modules" node  "$basedir/../ansi-html/bin/ansi-html" "$@"
  ret=$?
fi
exit $ret

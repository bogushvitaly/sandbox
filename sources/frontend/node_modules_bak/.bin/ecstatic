#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/lib/ecstatic.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/node_modules" "$basedir/node"  "$basedir/../ecstatic/lib/ecstatic.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ecstatic/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/lib/ecstatic.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/lib/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ecstatic/node_modules" node  "$basedir/../ecstatic/lib/ecstatic.js" "$@"
  ret=$?
fi
exit $ret

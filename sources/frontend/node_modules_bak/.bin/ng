#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/bin/ng/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/bin/ng/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/node_modules" "$basedir/node"  "$basedir/../@angular/cli/bin/ng" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/bin/ng/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/7.2.3/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/@angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/bin/ng/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/@angular/node_modules" node  "$basedir/../@angular/cli/bin/ng" "$@"
  ret=$?

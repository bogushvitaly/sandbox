#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/bin/start.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/bin/start.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/node_modules" "$basedir/node"  "$basedir/../start-server-and-test/bin/start.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/bin/start.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules/start-server-and-test/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/1.7.11/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/start-server-and-test/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/bin/start.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/start-server-and-test/node_modules" node  "$basedir/../start-server-and-test/bin/start.js" "$@"

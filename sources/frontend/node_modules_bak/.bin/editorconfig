#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/bin/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/bin/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/node_modules" "$basedir/node"  "$basedir/../editorconfig/bin/editorconfig" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/bin/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/0.15.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/bin/editorconfig/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/editorconfig/node_modules" node  "$basedir/../editorconfig/bin/editorconfig" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/bin/ndpack-image.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/bin/ndpack-image.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/node_modules" "$basedir/node"  "$basedir/../ndpack-image/bin/ndpack-image.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/bin/ndpack-image.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules/ndpack-image/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/3.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ndpack-image/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/bin/ndpack-image.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ndpack-image/node_modules" node  "$basedir/../ndpack-image/bin/ndpack-image.js" "$@"
  ret=$?
fi

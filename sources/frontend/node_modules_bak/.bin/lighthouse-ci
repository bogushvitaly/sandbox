#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/bin/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/bin/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/node_modules" "$basedir/node"  "$basedir/../lighthouse-ci/bin/cli" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/bin/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules/lighthouse-ci/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/1.3.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse-ci/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/bin/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse-ci/node_modules" node  "$basedir/../lighthouse-ci/bin/cli" "$@"
  ret=$?
fi
exit $ret

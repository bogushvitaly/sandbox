#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/bin/stylelint.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/bin/stylelint.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/node_modules" "$basedir/node"  "$basedir/../stylelint/bin/stylelint.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/bin/stylelint.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules/stylelint/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/9.10.1/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/bin/stylelint.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint/node_modules" node  "$basedir/../stylelint/bin/stylelint.js" "$@"
  ret=$?
fi
exit $ret

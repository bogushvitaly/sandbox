#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/scripts/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/scripts/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/node_modules" "$basedir/node"  "$basedir/../lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/scripts/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/lighthouse-core/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules/lighthouse/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/4.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/lighthouse/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/scripts/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/lighthouse-core/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/lighthouse/node_modules" node  "$basedir/../lighthouse/lighthouse-core/scripts/manual-chrome-launcher.js" "$@"

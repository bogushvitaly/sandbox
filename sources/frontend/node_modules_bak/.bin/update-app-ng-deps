#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/bin/update-app-ng-deps/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/bin/update-app-ng-deps/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/node_modules" "$basedir/node"  "$basedir/../nativescript-angular/bin/update-app-ng-deps" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/bin/update-app-ng-deps/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules/nativescript-angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/7.1.2/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/nativescript-angular/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/bin/update-app-ng-deps/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/nativescript-angular/node_modules" node  "$basedir/../nativescript-angular/bin/update-app-ng-deps" "$@"

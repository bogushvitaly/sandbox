#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/cli/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/cli/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/node_modules" "$basedir/node"  "$basedir/../ng-packagr/cli/main.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/cli/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules/ng-packagr/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/4.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ng-packagr/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/cli/main.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/cli/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ng-packagr/node_modules" node  "$basedir/../ng-packagr/cli/main.js" "$@"
  ret=$?
fi

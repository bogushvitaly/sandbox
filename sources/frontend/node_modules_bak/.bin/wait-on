#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/bin/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/bin/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/node_modules" "$basedir/node"  "$basedir/../wait-on/bin/wait-on" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/bin/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/3.2.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/bin/wait-on/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/wait-on/node_modules" node  "$basedir/../wait-on/bin/wait-on" "$@"
  ret=$?
fi
exit $ret

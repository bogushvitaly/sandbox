#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/node_modules" "$basedir/node"  "$basedir/../ngx-translate-extract/bin/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules/ngx-translate-extract/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/1.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/ngx-translate-extract/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/bin/cli.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/ngx-translate-extract/node_modules" node  "$basedir/../ngx-translate-extract/bin/cli.js" "$@"

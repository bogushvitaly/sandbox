#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/bin/sassgraph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/bin/sassgraph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/node_modules" "$basedir/node"  "$basedir/../sass-graph/bin/sassgraph" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/bin/sassgraph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/sass-graph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/bin/sassgraph/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/sass-graph/node_modules" node  "$basedir/../sass-graph/bin/sassgraph" "$@"
  ret=$?
fi
exit $ret

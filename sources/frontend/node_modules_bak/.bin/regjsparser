#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/bin/parser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/bin/parser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/node_modules" "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/bin/parser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules/regjsparser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/0.6.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/regjsparser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/bin/parser/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/regjsparser/node_modules" node  "$basedir/../regjsparser/bin/parser" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/bin/solid-auth-client.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/bin/solid-auth-client.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/node_modules" "$basedir/node"  "$basedir/../solid-auth-client/bin/solid-auth-client.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/bin/solid-auth-client.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules/solid-auth-client/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/2.2.14/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/solid-auth-client/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/bin/solid-auth-client.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/solid-auth-client/node_modules" node  "$basedir/../solid-auth-client/bin/solid-auth-client.js" "$@"

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/bin/check.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/bin/check.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/node_modules" "$basedir/node"  "$basedir/../stylelint-config-prettier/bin/check.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/bin/check.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules/stylelint-config-prettier/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/4.0.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/stylelint-config-prettier/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/bin/check.js/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/stylelint-config-prettier/node_modules" node  "$basedir/../stylelint-config-prettier/bin/check.js" "$@"

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/bin/allure/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/bin/allure/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/node_modules" "$basedir/node"  "$basedir/../allure-commandline/bin/allure" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/bin/allure/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules/allure-commandline/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/2.9.0/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/allure-commandline/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/.registry.npmjs.org/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules:/mnt/e/workspace/projects/sandbox/sources/node_modules:/mnt/e/workspace/projects/sandbox/node_modules:/mnt/e/workspace/projects/node_modules:/mnt/e/workspace/node_modules:/mnt/e/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/bin/allure/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/bin/node_modules:/mnt/e/workspace/projects/sandbox/sources/frontend/node_modules/allure-commandline/node_modules" node  "$basedir/../allure-commandline/bin/allure" "$@"
  ret=$?

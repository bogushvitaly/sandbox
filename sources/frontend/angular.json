{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss",
      "changeDetection": "OnPush"
    },
    "@nrwl/schematics:component": {
      "styleext": "scss",
      "changeDetection": "OnPush"
    }
  },
  "projects": {
    "libs": {
      "root": "libs",
      "sourceRoot": "libs",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.libs.ts",
            "tsConfig": "testing/tsconfig.libs.spec.json",
            "karmaConfig": "testing/karma.libs.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["testing/tsconfig.libs.json", "testing/tsconfig.libs.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "xplat": {
      "root": "xplat",
      "sourceRoot": "xplat",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.xplat.ts",
            "tsConfig": "testing/tsconfig.xplat.spec.json",
            "karmaConfig": "testing/karma.xplat.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["testing/tsconfig.xplat.json", "testing/tsconfig.xplat.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "web-application": {
      "root": "apps/web-application/",
      "sourceRoot": "apps/web-application/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "apps/web-application/webpack.config.js",
              "mergeStrategies": {
                "plugins": "append"
              }
            },
            "outputPath": "dist/apps/web-application",
            "index": "apps/web-application/src/index.html",
            "main": "apps/web-application/src/main.ts",
            "polyfills": "apps/web-application/src/polyfills.ts",
            "tsConfig": "apps/web-application/tsconfig.app.json",
            "assets": [{
                "glob": "favicon.ico",
                "input": "apps/web-application/src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/web-application/src/assets",
                "output": "/assets"
              },
              {
                "glob": "manifest.json",
                "input": "apps/web-application/src",
                "output": "/"
              },
              {
                "glob": "apple-touch-icon.png",
                "input": "apps/web-application/src",
                "output": "/"
              },
              {
                "glob": "robots.txt",
                "input": "apps/web-application/src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/ngx-auth-firebaseui/assets/",
                "output": "/assets"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "xplat/web/scss/_index.scss",
              "apps/web-application/src/styles.scss"
            ],
            "scripts": [{
              "input": "node_modules/document-register-element/build/document-register-element.js"
            }]
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/web-application/src/environments/environment.ts",
                "with": "apps/web-application/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }],
              "serviceWorker": true
            },
            "staging": {
              "fileReplacements": [{
                "replace": "apps/web-application/src/environments/environment.ts",
                "with": "apps/web-application/src/environments/environment.staging.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/dev-server:generic",
          "options": {
            "browserTarget": "web-application:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-application:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-application:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/web-application/src/test.ts",
            "polyfills": "apps/web-application/src/polyfills.ts",
            "tsConfig": "apps/web-application/tsconfig.spec.json",
            "karmaConfig": "apps/web-application/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/web-application/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/web-application/src/favicon.ico",
              "apps/web-application/src/assets",
              "apps/web-application/src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/web-application/tsconfig.app.json", "apps/web-application/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/web-application-server",
            "main": "apps/web-application/src/main.server.ts",
            "tsConfig": "apps/web-application/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/web-application/src/environments/environment.ts",
                "with": "apps/web-application/src/environments/environment.prod.ts"
              }],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            },
            "udk": {
              "fileReplacements": [{
                "replace": "apps/web-application/src/main.server.ts",
                "with": "apps/web-application/src/main.udk.ts"
              }]
            }
          }
        },
        "udk": {
          "builder": "udk:udk-builder",
          "options": {
            "browserTarget": "web-application:build",
            "serverTarget": "web-application:server:udk"
          }
        }
      }
    },
    "web-application-e2e": {
      "root": "apps/web-application-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/web-application-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "web-application-ssr": {
      "root": "apps/web-application-ssr",
      "sourceRoot": "apps/web-application-ssr/src",
      "projectType": "application",
      "prefix": "web-application-ssr",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/builders:node-build",
          "options": {
            "outputPath": "dist/apps/web-application-ssr",
            "main": "apps/web-application-ssr/src/main.ts",
            "tsConfig": "apps/web-application-ssr/tsconfig.app.json",
            "assets": ["apps/web-application-ssr/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "fileReplacements": [{
                "replace": "apps/web-application-ssr/src/environments/environment.ts",
                "with": "apps/web-application-ssr/src/environments/environment.prod.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/builders:node-execute",
          "options": {
            "buildTarget": "web-application-ssr:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/web-application-ssr/tsconfig.app.json", "apps/web-application-ssr/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/web-application-ssr/jest.config.js",
            "tsConfig": "apps/web-application-ssr/tsconfig.spec.json",
            "setupFile": "apps/web-application-ssr/src/test-setup.ts"
          }
        }
      }
    },
    "nest-application": {
      "root": "apps/nest-application",
      "sourceRoot": "apps/nest-application/src",
      "projectType": "application",
      "prefix": "nest-application",
      "schematics": {},
      "architect": {}
    },
    "fan-menu": {
      "root": "libs/fan-menu",
      "sourceRoot": "libs/fan-menu/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/fan-menu/tsconfig.lib.json",
            "project": "libs/fan-menu/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/fan-menu/tsconfig.lib.json", "libs/fan-menu/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/fan-menu/jest.config.js",
            "tsConfig": "libs/fan-menu/tsconfig.spec.json",
            "setupFile": "libs/fan-menu/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "storybook": {
      "root": "apps/storybook/",
      "sourceRoot": "apps/storybook/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/storybook",
            "index": "apps/storybook/src/index.html",
            "main": "apps/storybook/src/main.ts",
            "polyfills": "apps/storybook/src/polyfills.ts",
            "tsConfig": "apps/storybook/tsconfig.app.json",
            "assets": ["apps/storybook/src/favicon.ico", "apps/storybook/src/assets"],
            "styles": ["apps/storybook/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/storybook/src/environments/environment.ts",
                "with": "apps/storybook/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "storybook:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "storybook:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "storybook:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/storybook/tsconfig.app.json", "apps/storybook/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/storybook/jest.config.js",
            "tsConfig": "apps/storybook/tsconfig.spec.json",
            "setupFile": "apps/storybook/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nstudio/schematics"
  },
  "defaultProject": "web-application"
}

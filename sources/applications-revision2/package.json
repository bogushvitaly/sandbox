{
  "name": "applications",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "browser": {
    "crypto": false,
    "fs": false,
    "os": false,
    "path": false,
    "stream": false
  },
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "analyze": "npx npm-run-all -p portal:web:bundle",
    "build": "npx npm-run-all -p portal:build:all storybook:build",
    "clean": "npx npm-run-all -p clean:applications clean:functions upgrade",
    "clean:applications": "npx rimraf hooks node_modules yarn.lock",
    "clean:functions": "npx rimraf hooks functions/node_modules functions/yarn.lock",
    "clean:shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "dep-graph": "nx dep-graph --all --file=reports/deps-graph/index.html",
    "dev": "dotenv-extended yarn ng serve --hmr",
    "docs:compodoc": "compodoc -p ./tsconfig.json -d reports/compodoc",
    "docs:wiki": "hads ./docs -o",
    "e2e": "node -h",
    "format": "prettier --write ./{apps,libs,xplat,tools}/**/*.{ts,js,json,scss,css,html}",
    "format:check": "prettier --list-different ./{apps,libs,xplat,tools}/**/*.{ts,js,json,scss,css,html}",
    "help": "nx help",
    "postinstall": "npx npm-run-all -p patch:jest patch:babylon",
    "lint": "yarn lint:html && yarn lint:scss --fix && yarn ng lint --fix && nx lint",
    "lint:check": "yarn lint:html && yarn lint:scss && yarn ng lint && nx lint",
    "lint:html": "htmlhint ./{apps,libs,xplat}/**/*.html",
    "lint:scss": "stylelint ./{apps,libs,xplat}/**/*.scss --syntax scss",
    "nest:build": "tsc -p apps/nest",
    "nest:serve": "ts-node -P apps/nest/tsconfig.json apps/nest/src/main.ts",
    "nest:start": "npx npm-run-all -p serve:nest",
    "nest:test": "jest --config=apps/nest/jest.json",
    "nest:test:coverage": "jest --config=apps/nest/jest.json --coverage",
    "nest:test:e2e": "jest --config=apps/nest/e2e/jest-e2e.json --forceExit",
    "nest:test:e2e:watch": "jest --config=apps/nest/e2e/jest-e2e.json --watch",
    "nest:test:watch": "jest --config=apps/nest/jest.json --watch",
    "ng": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng",
    "patch:agm": "babel node_modules/@agm/core -d node_modules/@agm/core --presets @babel/preset-env",
    "patch:babylon": "npx npm-run-all -s patch:babylon:convert patch:babylon:sync",
    "patch:babylon:convert": "babel tools/custom-packages/babylonjs-mesh-writer/meshwriter.min.js --out-file tools/custom-packages/babylonjs-mesh-writer/babylonjs.meshWriter.js --plugins=transform-amd-to-commonjs",
    "patch:babylon:sync": "sync-files tools/custom-packages/babylonjs-mesh-writer node_modules/babylonjs-mesh-writer",
    "patch:jest": "replace-in-file jest-resolve/build/default_resolver jest-resolve/build/defaultResolver ./node_modules/@nrwl/builders/plugins/jest/resolver.js",
    "portal:api:build": "dotenv-extended yarn ng build portal-api --configuration=production",
    "portal:api:serve": "dotenv-extended yarn ng serve portal-api --port=7771",
    "portal:api:start": "dotenrv-extended node dist/apps/portal-api/main.js",
    "portal:build:all": "dotenv-extended npx npm-run-all -p portal:api:build portal:static:build portal:web:build",
    "portal:start:all": "dotenv-extended npx npm-run-all -p portal:api:start portal:static:start portal:web:start",
    "portal:static:build": "dotenv-extended yarn ng build portal-static --configuration=production",
    "portal:static:serve": "dotenv-extended yarn ng serve portal-static --port=7771",
    "portal:static:start": "dotenv-extended node dist/apps/portal-static/main.js",
    "portal:web:build": "dotenv-extended yarn ng build --prod",
    "deploy:firebase": "firebase deploy --only hosting",
    "portal:web:build:udk": "dotenv-extended yarn ng run portal-web:udk:production",
    "portal:web:bundle": "npx npm-run-all -s portal:web:bundle:prepare portal:web:bundle:analyze",
    "portal:web:bundle:analyze": "webpack-bundle-analyzer dist/apps/portal-web/stats.json --no-open --mode static --report reports/apps/web-application/bundle/index.html",
    "portal:web:bundle:prepare": "dotenv-extended yarn ng build portal-web --prod --stats-json",
    "portal:web:e2e": "start-test portal:web:start http-get://localhost:4000 e2e.web.application.report",
    "portal:web:e2e:codecept": "dotenv-extended codeceptjs run --config apps/portal-web-e2e/codecept.conf.js",
    "portal:web:e2e:codecept:shell": "cd apps/portal-web-e2e && codeceptjs shell",
    "portal:web:e2e:codecept:steps": "codeceptjs gherkin:snippets --config apps/portal-web-e2e/codecept.conf.js",
    "portal:web:e2e:report": "yarn portal:web:e2e:codecept --plugins stepByStepReport --plugins allure --reporter mocha-multi",
    "portal:web:e2e:report:allure": "allure generate reports/apps/portal-web/codecept --clean --report-dir reports/apps/portal-web/allure",
    "portal:web:e2e:watch": "wait-on http-get://localhost:4200 && nodemon --exec \"yarn portal:web:e2e:codecept:steps && yarn portal:web:e2e:codecept\"",
    "portal:web:serve": "dotenv-extended yarn udkc --project portal-web",
    "portal:web:serve:spa": "dotenv-extended yarn ng serve --project portal-web --hmr",
    "portal:web:start": "dotenv-extended node dist/apps/portal-web/main.js",
    "scaffold:codecept": "cd apps/portal-web-e2e && codeceptjs init && codeceptjs gherkin:init && codeceptjs def .",
    "start": "cross-env npm-run-all -p --print-label start.web.application.api start.static.content.server start.web.application.hmr",
    "start:portal:web": "yarn ng serve portal-web --open",
    "stencil:build": "cd apps/stencil && yarn build",
    "stencil:prepare": "cd apps/stencil && npm install",
    "stencil:start": "cd apps/stencil && yarn start",
    "stencil:test": "cd apps/stencil && yarn test",
    "stencil:test:watch": "cd apps/stencil && yarn test.watch",
    "storybook": "start-storybook -s apps/storybook/src/assets -p 6006",
    "storybook:build": "build-storybook -s apps/storybook/src/assets -o reports/storybook",
    "test": "npx npm-run-all -s test:coverage test:combine",
    "test:combine": "istanbul-combine -d reports/coverage -p summary -r lcov -r html coverage/libs-all/coverage-final.json coverage/xplat/coverage-final.json",
    "test:coverage": "yarn ng test --code-coverage",
    "test:watch": "yarn ng test --watch",
    "udkc": "node --max-old-space-size=8192 ./node_modules/udk/angular/bin/ng-udkc.js",
    "update": "yarn ng update @nrwl/schematics",
    "update:check": "yarn ng update --help",
    "upgrade": "npx npm-run-all -p upgrade:applications upgrade:functions",
    "upgrade:applications": "npx sort-package-json && npx npm-check-updates -u -x typescript --removeRange && yarn",
    "upgrade:functions": "cd functions && npx sort-package-json && npx npm-check-updates -u -x typescript --removeRange && yarn",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
